{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "methods": ["GET", "POST", "PUT", "DELETE"],  
        "headers": {
          "X-Custom-Header": "YourValue"  
        },
        "dest": "index.js"
      }
    ]
  }
  